{
  "jedi.enable": true,
  "jedi.startupMessage": false,
  "jedi.markupKindPreferred": "plaintext",
  "jedi.trace.server": "off",
  "jedi.jediSettings.autoImportModules": [],
  "jedi.jediSettings.caseInsensitiveCompletion": true,
  "jedi.jediSettings.debug": false,
  "jedi.executable.command": "jedi-language-server",
  "jedi.executable.args": [],
  "jedi.codeAction.nameExtractFunction": "jls_extract_def",
  "jedi.codeAction.nameExtractVariable": "jls_extract_var",
  "jedi.completion.disableSnippets": false,
  "jedi.completion.resolveEagerly": false,
  "jedi.completion.ignorePatterns": [],
  "jedi.diagnostics.enable": true,
  "jedi.diagnostics.didOpen": true,
  "jedi.diagnostics.didChange": true,
  "jedi.diagnostics.didSave": true,
  "jedi.hover.enable": true,
  "jedi.hover.disable.keyword.all": false,
  "jedi.hover.disable.keyword.names": [],
  "jedi.hover.disable.keyword.fullNames": [],
  "jedi.workspace.extraPaths": [],
  "jedi.workspace.symbols.maxSymbols": 20,
  "jedi.workspace.symbols.ignoreFolders": [
    ".nox",
    ".tox",
    ".venv",
    "__pycache__",
    "venv"
  ],
  "clangd.enabled":true,
  "clangd.arguments":[],
  "clangd.checkUpdates": false,
  "clangd.disableCompletion": false,
  "clangd.disableDiagnostics":false,
  "clangd.disableSnippetCompletion": false,
  "clangd.compilationDatabasePath": "",
  "clock.enable": false,
  "clock.color": "",
  "clock.winblend": 10,
  "clock.top": 1,
  "clock.right": 1,
  "clock.trace.server": "off",
  "cmake.cmakePath":"/path/to/cmake",
  "cmake.formatter_args":[],
  "cmake.lsp.enable":true,
  "cmake.lsp.serverPath":"/path/to/cmake-language-server",
  "cmake.lsp.buildDirectory":"build",
  "languageserver": {
      "cmake" : {
          "command" : "cmake-language-server",
          "filetypes": ["cmake"],
          "rootPatterns":[
              "build/"
          ],
          "initializationOptions": {
              "buildDirectory":"build"
          }
      },
      "bash": {
        "command": "bash-language-server",
        "args": ["start"],
        "filetypes": ["sh"],
        "ignoredRootPaths": ["~"]
      }
  },
  "diagnostic-languageserver.filetypes": {
      "python": "pylint",
      "vim":"vint",
      "markdown":["write-good","markdownlint"],
      "sh":"shellcheck",
      "php":["phpstan","psalm"],
      "yaml":["yamllint"],
      "cmake":["cmake-lint","cmakelint"],
      "systemd":"systemd-analyze"
  },
  "diagnostic-languageserver.linters":{
      "pylint":{
          "sourceName": "pylint",
          "command": "pylint",
          "debounce":"100",
          "args": [
              "--output-format",
              "text",
              "--score",
              "no",
              "--msg-template",
              "'{line}:{column}:{category}:{msg} ({msg_id}:{symbol})'",
              "%file"
          ],
          "formatPattern":[
              "^(\\d+?):(\\d+?):([a-z]+?):(.*)$",
              {
                  "line": 1,
                  "column": 2,
                  "endColumn": 2,
                  "security": 3,
                  "message": 4
              }
          ],
          "rootPatterns": ["pyproject.toml","setup.py",".git"],
          "securities": {
              "informational":"hint",
              "refactor": "info",
              "convention": "warning",
              "error": "error",
              "fatal": "error"
          },
          "offsetColumn": 1,
          "offsetColumnEnd": 1,
          "formatLines": 1
      }
  },
  "css.enable": true,
  "cssmodules.camelCase":true,
  "cssmodules.hintName":"cssmodules",
  "emmet.showExpandedAbbreviation":"always",
  "eslint.enable":true,
  "eslint.execArgv": [],
  "eslint.fixOnSaveTimeout": 1000,
  "eslint.packageManager": "npm",
  "eslint.alwaysShowStatus": false,
  "eslint.nodeEnv":  null,
  "eslint.nodePath": "/path/to/node",
  "eslint.options": {},
  "eslint.trace.server": "off",
  "eslint.run": "onType",
  "eslint.autoFixOnSave":false,
  "eslint.quiet": false,
  "eslint.onIgnoredFiles":"off",
  "eslint.useESLintClass": false,
  "eslint.probe": ["javascript","javascriptreact","typescript","typescriptreact","html","vue","markdown"],
  "eslint.runtime": null,
  "eslint.debug": false,
  "eslint.codeAction.disableRuleComment": {"enable":true,"location":"separateLine"},
  "eslint.codeAction.showDocumentation": {"enable":true},
  "eslint.codeActionsOnSave.mode":"all",
  "eslint.format.enable":false,
  "eslint.lintTask.options": ["."],
  "htmldjango.enable":true,
  "htmldjango.builtin.pythonPath":"/path/to/python3",
  "htmldjango.djhtml.commandPath":"/path/to/node/djhtml",
  "htmldjango.djlint.commandPath":"/path/to/node/djlint",
  "htmldjango.djlint.enableLint":true,
  "sh.enable": true,
  "sqlfluff.enable": true,
  "sqlfluff.lintOnOpen": true,
  "sqlfluff.lintOnChange":true,
  "sqlfluff.lintOnSave": true,
  "sqlfluff.commandPath":"/path/to/node/sqlfluff",
  "sqlfluff.builtin.pythonPath":"~/.config/coc/extensions/coc-sqlfluff-data/sqlfluff/env/bin",
  "tsserver.enable":true,
  "tsserver.locale": "/path/to/node/bin/typescript-language-server",
  "tsserver.tsdk": "/path/to/node/lib/node_modules/corepack/node_modules/typescript/lib",
  "tsserver.npm": "/path/to/node/bin/npm"
}
